<window id="4001" type="dialog">
    <defaultcontrol always="false">1000</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <backgroundcolor>ff000000</backgroundcolor>
    <visible>true</visible>
    <zorder>0</zorder>
    <previouswindow></previouswindow>
    <useincludes>false</useincludes>


    <controls>
        <!-- Main background image -->
        <control type="image">
            <description>Main background image</description>
            <posx></posx>
            <posy></posy>
            <width>1280</width>
            <height>720</height>
            <visible>true</visible>
            <colordiffuse>FFFFFFFF</colordiffuse>
            <fadetime>200</fadetime>
            <texture border="5" flipy="false" flipx="false">main-bg.png</texture>
            <aspectratio>stretch</aspectratio>
        </control>
        <control type="group">
            <description>Main Menu</description>
                <defaultcontrol>210</defaultcontrol>
                <visible>true</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>1280</width>
                <height>128</height>
            <control type="label" id=1>
                <posy>10</posy>
                <height>25</height>
                <label>Qobuz powered search</label>
            </control>
            <control type="radiobutton" id="2">
                <description>Artist</description>
                <type>radiobutton</type>
                <visible>true</visible>
                <label>Artist</label>
                <align>left</align>
                <aligny>center</aligny>
                <textoffsetx>4</textoffsetx>
                <textoffsety>5</textoffsety>
                <pulseonselect>false</pulseonselect>
                <onfocus>-</onfocus>
                <onunfocus>-</onunfocus>
                <onup>2</onup>
                <ondown>3</ondown>
                <onleft>1</onleft>
                <onright>1</onright>
                <posy>10</posy>
                <height>25</height>
                <!-- <selected>True</selected> -->
            </control>
